#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
    {
       int n,x,i;
       cin>>n>>x;
       int a[n];
       for(i=0;i<n;i++)
        cin>>a[i];
       int ind=-1;
       for(i=0;i<n;i++)
       {
           if(a[i]<=x)
            ind=i;
       }
       cout<<ind<<endl;
    }
	return 0;
}
