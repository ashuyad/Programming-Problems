#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
    {
       int n,i;
       cin>>n;
       int a[100]={0};
       int count=0;
       for(i=1;i<=n;i++)
       {
          int x;
          cin>>x;
          a[x]++;
       }
       for(i=1;i<=100;i++)
       {
           if(a[i]==1)
           {
               count=i;
            break;
           }
       }
       cout<<count<<endl;
    }
	return 0;
}
