#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t,i;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        i=0;
int flag=0;
       while(i<s.length())
        {


                if(s[i]=='L' &&s[i+1]=='I'&&s[i+2]=='E')
            {
                if(flag!=0)
                {cout<<" ";}

                i+=3;
                flag=0;
            }
            else
            {
                cout<<s[i];
            i++;
            flag=1;

            }


        }
        cout<<endl;
    }
	return 0;
}
